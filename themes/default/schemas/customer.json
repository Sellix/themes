{
  "Customer Dashboard Analytic Items": {
    "id": "Customer Dashboard Analytic Items",
    "name": "Customer Dashboard Analytic Items",
    "settings": {
      "schema": [
        {
          "id": "customer_dashboard_analytic_item_background",
          "type": "select",
          "label": "Background color",
          "options": [
            {
              "title": "Main",
              "value": "background_main_color"
            },
            {
              "title": "Accent (Light)",
              "value": "background_accent_light"
            },
            {
              "title": "Accent (Dark)",
              "value": "background_accent_dark"
            },
            {
              "title": "Transparent",
              "value": "transparent"
            }
          ],
          "defaultValue": "background_accent_dark"
        },

        {
          "id": "customer_dashboard_analytic_item_show_border",
          "type": "checkbox",
          "label": "Show border",
          "defaultValue": false
        },
        {
          "id": "customer_dashboard_analytic_item_border_radius",
          "type": "range",
          "max": 2,
          "min": 0,
          "step": 0.25,
          "unit": "rem",
          "label": "Border radius",
          "defaultValue": 0.5
        },
        {
          "id": "customer_dashboard_analytic_item_shadow",
          "type": "select",
          "label": "Drop shadow",
          "options": [
            {
              "title": "None",
              "value": "none"
            },
            {
              "title": "Variant 1",
              "value": "0 1px 3px rgba(7, 14, 35, 0.06), 0px 2px 8px -1px rgba(7, 14, 35, 0.05)"
            },
            {
              "title": "Variant 2",
              "value": "0px 2px 4px -1px rgba(0,0,0,0.2),0px 4px 5px 0px rgba(0,0,0,0.14),0px 1px 10px 0px rgba(0,0,0,0.12)"
            },
            {
              "title": "Variant 3",
              "value": "0px 3px 5px -1px rgba(0,0,0,0.2),0px 6px 10px 0px rgba(0,0,0,0.14),0px 1px 18px 0px rgba(0,0,0,0.12)"
            }
          ],
          "defaultValue": "0 1px 3px rgba(7, 14, 35, 0.06), 0px 2px 8px -1px rgba(7, 14, 35, 0.05)"
        },
        {
          "id": "customer_dashboard_analytic_item_size",
          "type": "select",
          "label": "Size",
          "options": [
            {
              "title": "Compact",
              "value": 50
            },
            {
              "title": "Wide",
              "value": 100
            }
          ],
          "defaultValue": 50
        },
        {
          "id": "customer_dashboard_analytic_item_font_color",
          "type": "select",
          "label": "Font color",
          "options": [
            {
              "title": "Light",
              "value": "light_font_color"
            },
            {
              "title": "Dark",
              "value": "dark_font_color"
            },
            {
              "title": "Brand",
              "value": "brand_color"
            }
          ],
          "defaultValue": "light_font_color"
        },

        {
          "id": "customer_dashboard_analytic_padding_horiz",
          "max": 24,
          "min": 0,
          "step": 1,
          "type": "range",
          "unit": "px",
          "label": "Container padding horizontal",
          "defaultValue": 16
        },
        {
          "id": "customer_dashboard_analytic_padding_vert",
          "max": 24,
          "min": 0,
          "step": 1,
          "type": "range",
          "unit": "px",
          "label": "Container padding vertical",
          "defaultValue": 16
        },

        {
          "id": "customer_dashboard_analytic_items_per_row_wide",
          "type": "text",
          "label": "Items per row (wide)",
          "defaultValue": "2"
        },
        {
          "id": "customer_dashboard_analytic_items_per_row_normal",
          "type": "text",
          "label": "Items per row (normal)",
          "defaultValue": "2"
        },
        {
          "id": "customer_dashboard_analytic_items_per_row_narrow",
          "type": "text",
          "label": "Items per row (narrow)",
          "defaultValue": "1"
        }
      ]
    },
    "file": {
      "checksum": "checksum of file",
      "id": "blocks/customer/dashboardAnalyticItems.njk"
    }
  },
