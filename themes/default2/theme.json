  "name": "Default 2",
  "category": "Digital Products",
  "preview_url": "https://cdn.sellix.io/static/previews/themes/default.png",

  "settings": {
    "schema_order": [
      "color_schema",
      "background",
      "page_loader",
      "typography",
      "locale"
    ],
    "schema": {
      "background": [
        {
          "id": "background_image",
          "type": "select-image",
          "label": "Image",
          "defaultValue": null
        }
      ],
      "color_schema": [
        {
          "id": "dark_mode",
          "type": "checkbox",
          "label": "Enable Dark mode",
          "defaultValue": false
        }
      ],
      "dark_colors": [
        {
          "id": "background_main_color",
          "type": "color-picker",
          "label": "Main",
          "defaultValue": "#FFFFFF"
        }
      ],
      "light_colors": [
        {
          "id": "background_main_color",
          "type": "color-picker",
          "label": "Main",
          "defaultValue": "#FFFFFF"
        }
      ],
      "locale": [
        {
          "id": "default",
          "type": "text",
          "label": "Default locale",
          "defaultValue": "en"
        }
      ],
      "page_loader": [
        {
          "id": "enabled",
          "type": "checkbox",
          "label": "Enable page loader",
          "defaultValue": true
        },
        {
          "id": "color",
          "type": "color-picker",
          "label": "Page loader color",
          "defaultValue": "#5F40E5"
        },
        {
          "id": "height",
          "max": 20,
          "min": 1,
          "step": 1,
          "type": "range",
          "unit": "px",
          "label": "Page loader height",
          "defaultValue": 2
        }
      ],
      "typography": [
        {
          "id": "font",
          "type": "select-font",
          "label": "Font",
          "defaultValue": {
            "fontFamily": "Inter",
            "href": "https://fonts.googleapis.com/css2?family=Inter:wght@100;200;300;400;500;600;700;800;900&display=swap",
            "systemFont": "sans-serif"
          }
        }
      ]
    },
    "properties": {
      "background": {
        "background_image": null
      },
      "color_schema": {
        "dark_mode": false
      },
      "dark_colors": {
        "background_main_color": "#0A1730"
      },
      "light_colors": {
        "background_main_color": "#FFFFFF"
      },
      "typography": {
        "font": {
          "fontFamily": "Inter",
          "href": "https://fonts.googleapis.com/css2?family=Inter:wght@100;200;300;400;500;600;700;800;900&display=swap",
          "systemFont": "sans-serif"
        }
      },
      "locale": {
        "default": "en"
      },
      "page_loader": {
        "enabled": true,
        "color": "#5F40E5",
        "height": 2
      }
    }
  },

